[General]
PidDirectory=@CONF_GENERAL_PID_DIRECTORY@
LogServiceStatus=@CONF_GENERAL_LOG_SERVICE_STATUS@
ConnectTimeout=@CONF_GENERAL_CONNECT_TIMEOUT@

[Watchdog]
Timeout=@CONF_WATCHDOG_TIMEOUT@
FailureMode=@CONF_FAILURE_MODE@

[Dynamic Services]
ExecPrefix=@CONF_DYNAMIC_SERVICES_EXEC_PREFIX@
LaunchTimeout=@CONF_DYNAMIC_SERVICES_LAUNCH_TIMEOUT@

[Security]
Enabled=@CONF_SECURITY_ENABLED@
MonitorExePath=@CONF_SECURITY_MONITOR_EXE_PATH@
JsServiceExePath=@CONF_SECURITY_JS_SERVICE_EXE_PATH@
AllowNullOutboundByDefault=@CONF_SECURITY_ALLOW_NULL_OUTBOUND_BY_DEFAULT@
ContainersDirectories=@CONF_SECURITY_CONTAINERS_DIRECTORIES@
ManifestsDirectories=@CONF_SECURITY_MANIFESTS_DIRECTORIES@
ManifestsVolatileDirectories=@CONF_SECURITY_MANIFESTS_VOLATILE_DIRECTORIES@
DevmodeCertificate=@CONF_SECURITY_DEVMODE_CERTIFICATE@
DefaultDevmodeCertificate=@CONF_SECURITY_DEFAULT_DEVMODE_CERTIFICATE@
